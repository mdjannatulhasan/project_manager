{
    "semi": false,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 80,
    "plugins": [
        "@prettier/plugin-ruby",
        "prettier-plugin-erb"
    ],
    "rubyHashLabel": true,
    "rubyModifier": true,
    "rubySingleQuote": false,
    "rubyToProc": false,
    "rubyArrayLiteral": true
}